{
  "$schema":"https://json-schema.org/draft/2019-09/schema#",


  "type":"object",
  "properties":{
    "inventory_name":{
      "type":"string"
    },
    "inventory_id":{
      "type":"string"
    },
    "inventory_page_type":{
      "type":"string"
    },
    "inventory_page_section":{
      "type":"string"
    },
    "inventory_page_data":{
      "type":"string"
    },
    "inventory_lang":{
      "type":"string"
    },
    "visibility":{
      "type":"string"
    },
    "broker_enabled":{
      "type":"boolean"
    },
    "widget_data":{
      "type":"object",
      "properties":{
        "wid":{
          "type":"string"
        },
        "positionInParent":{
          "type":"integer"
        },
        "parameters":{
          "type":"object",
          "properties":{
            "title":{
              "type":"string"
            },
            "description":{
              "type":"string"
            },
            "keywords":{
              "type":"string"
            }
          },
          "required":[
            "description",
            "keywords",
            "title"
          ]
        },
        "children":{
          "type":"array"
        },
        "wtype":{
          "type":"string"
        }
      },
      "required":[
        "children",
        "parameters",
        "positionInParent",
        "wid",
        "wtype"
      ]
    },
    "html":{
      "type":"null"
    },
    "weightage_in_page":{
      "type":"integer"
    },
    "dimensions":{
      "type":"object",
      "properties":{
        "mobile":{
          "type":"object",
          "properties":{
            "_cls":{
              "type":"string"
            }
          },
          "required":[
            "_cls"
          ]
        },
        "desktop":{
          "type":"object",
          "properties":{
            "_cls":{
              "type":"string"
            }
          },
          "required":[
            "_cls"
          ]
        }
      },
      "required":[
        "desktop",
        "mobile"
      ]
    },
    "position":{
      "type":"integer"
    }
  },
  "required":[
    "broker_enabled",
    "html",
    "inventory_id",
    "inventory_lang",
    "inventory_name",
    "inventory_page_data",
    "inventory_page_section",
    "inventory_page_type",
    "position",
    "visibility",
    "weightage_in_page",
    "widget_data"
  ]



}